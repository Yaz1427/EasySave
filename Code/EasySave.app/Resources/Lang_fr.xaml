<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- Window -->
    <sys:String x:Key="AppTitle">EasySave v2.0</sys:String>

    <!-- Tabs -->
    <sys:String x:Key="TabJobs">Travaux de sauvegarde</sys:String>
    <sys:String x:Key="TabCreate">Creer un travail</sys:String>
    <sys:String x:Key="TabEdit">Modifier un travail</sys:String>
    <sys:String x:Key="TabLogs">Logs</sys:String>
    <sys:String x:Key="TabSettings">Parametres</sys:String>

    <!-- Job list -->
    <sys:String x:Key="LblName">Nom</sys:String>
    <sys:String x:Key="LblSource">Source</sys:String>
    <sys:String x:Key="LblTarget">Cible</sys:String>
    <sys:String x:Key="LblType">Type</sys:String>
    <sys:String x:Key="LblFull">Complete</sys:String>
    <sys:String x:Key="LblDifferential">Differentielle</sys:String>

    <!-- Buttons -->
    <sys:String x:Key="BtnRun">Executer</sys:String>
    <sys:String x:Key="BtnRunAll">Tout executer</sys:String>
    <sys:String x:Key="BtnDelete">Supprimer</sys:String>
    <sys:String x:Key="BtnCreate">Creer</sys:String>
    <sys:String x:Key="BtnSave">Enregistrer</sys:String>
    <sys:String x:Key="BtnBrowse">Parcourir...</sys:String>
    <sys:String x:Key="BtnEdit">Modifier</sys:String>
    <sys:String x:Key="BtnSaveEdit">Enregistrer</sys:String>
    <sys:String x:Key="BtnCancel">Annuler</sys:String>
    <sys:String x:Key="BtnStop">Arreter</sys:String>

    <!-- Create form -->
    <sys:String x:Key="LblJobName">Nom du travail :</sys:String>
    <sys:String x:Key="LblSourceDir">Repertoire source :</sys:String>
    <sys:String x:Key="LblTargetDir">Repertoire cible :</sys:String>
    <sys:String x:Key="LblBackupType">Type de sauvegarde :</sys:String>

    <!-- Settings -->
    <sys:String x:Key="LblLanguage">Langue :</sys:String>
    <sys:String x:Key="LblLogFormat">Format des logs :</sys:String>
    <sys:String x:Key="LblEncryptionMode">Mode de cryptage :</sys:String>
    <sys:String x:Key="LblEncryptExt">Extensions a crypter :</sys:String>
    <sys:String x:Key="LblEncryptExtHint">ex: .txt; .docx; .pdf</sys:String>
    <sys:String x:Key="LblBusinessSoftware">Processus logiciel metier :</sys:String>
    <sys:String x:Key="LblBusinessSoftwareHint">ex: CalculatorApp</sys:String>
    <sys:String x:Key="LblCryptoSoftPath">Chemin CryptoSoft :</sys:String>
    <sys:String x:Key="LblSaveSettings">Enregistrer les parametres</sys:String>

    <!-- Status -->
    <sys:String x:Key="LblProgress">Progression :</sys:String>
    <sys:String x:Key="LblStatus">Statut :</sys:String>
    <sys:String x:Key="LblCurrentJob">Travail en cours :</sys:String>
    <sys:String x:Key="LblNoJobs">Aucun travail de sauvegarde configure.</sys:String>

    <!-- Logs -->
    <sys:String x:Key="LblLogFile">Fichier de log :</sys:String>
    <sys:String x:Key="BtnRefresh">Actualiser</sys:String>
    <sys:String x:Key="BtnOpenFolder">Ouvrir le dossier</sys:String>
    <sys:String x:Key="LblTimestamp">Horodatage</sys:String>
    <sys:String x:Key="LblLogJobName">Nom du travail</sys:String>
    <sys:String x:Key="LblSourcePath">Source</sys:String>
    <sys:String x:Key="LblTargetPath">Cible</sys:String>
    <sys:String x:Key="LblFileSize">Taille (octets)</sys:String>
    <sys:String x:Key="LblTransferTime">Transfert (ms)</sys:String>
    <sys:String x:Key="LblEncryptTime">Chiffrement (ms)</sys:String>
    <sys:String x:Key="LblNoLogs">Aucun fichier de log trouve.</sys:String>

</ResourceDictionary>
